        <apex:pageBlock title="Edit BioBriefing">
            <apex:pageBlockSection columns="2">		
                <apex:inputField value="{!BioBriefing__c.Name__c}"/>
                <apex:inputField value="{!BioBriefing__c.Account_Name__c}"/>
                <apex:inputField value="{!BioBriefing__c.Department__c}"/>
                <apex:inputField value="{!BioBriefing__c.Birth_Date__c}"/>
