    </validationRules>
    <validationRules>
        <fullName>Age_between_21_to_30</fullName>
        <active>true</active>
        <errorConditionFormula>Age__c  &lt; 21 || Age__c  &gt; 31</errorConditionFormula>
        <errorMessage>Employee age should be between 21 - 30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOB_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(YEAR( TODAY() ) -   YEAR( Date_Of_Birth__c )) &lt;= 18</errorConditionFormula>
        <errorDisplayField>Date_Of_Birth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 18 years of Age.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_Length</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(TEXT( SSN__c )) &gt; 10</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>SSN number length should be less than 10</errorMessage>
    </validationRules>
</CustomObject>
